{
	"schemaVersion": 1,
	"id": "creaturechat",
	"version": "${version}",
	"name": "Creature Chat",
	"description": "Elevate your Minecraft experiences with Creature Chat, a cutting-edge mod that brings intelligent, dynamic conversation to Minecraft creatures! We harnesses the power of GPT (Generative Pre-trained Transformer) to make your world more interactive and communicative.",
	"authors": [
		"Jonathan Thomas <jonathan@openshot.org>",
		"owlmaddie <owlmaddie@gmail.com>"
	],
	"contact": {
		"homepage": "http://owlmaddie.com",
		"sources": "http://gitlab.openshot.org/minecraft/creature-chat"
	},
	"license": "GPLv3",
	"icon": "assets/creaturechat/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.owlmaddie.ModInit"
		],
		"client": [
			"com.owlmaddie.ClientInit"
		]
	},
	"mixins": [
		"creaturechat.mixins.json",
		{
			"config": "creaturechat.client.mixins.json",
			"environment": "client"
		}
	],
	"accessWidener": "creaturechat.accesswidener",
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
